#include <stdio.h>
#include <string.h>
int main(){
    char str[100] , str1[200];
    printf ("Enter the first string:");
    scanf("%s",str);
    printf("Enter the second string:");
    scanf("%s",str1);
    if (strcmp(str,str1)==0)
     printf("string are equal\n");
     else
     printf("string are not equal\n");
     return 0;
}
